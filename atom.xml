---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
	<feed xmlns="http://www.w3.org/2005/Atom">
	<title>La Jornada Zacatecas</title>
	<link href="http://ljzac.dev/atom.xml" rel="self"/>
	<link href="http://ljzac.dev/"/>
	<id>http://ljzac.dev/</id>
	<updated>{{ .site.time | date_to_xmlschema }}</updated>
	{{ range .s.PagesForCategory "MinuteByMinute" 20 }}
	<entry>
		<title>{{ .title }}</title>
		<link href="http://ljzac.dev/{{ .url }}" />
		<updated>{{ .date | date_to_xmlschema }}</updated>
		<id>http://ljzac.dev/{{ .url }}</id>
		<content type="html">{{ .content | html }}</content>
    <category>{{.GetCategories}}</category>
	</entry>
	{{ end }}
</feed>
