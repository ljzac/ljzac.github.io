		<id>http://ljz.mx/{{ .url }}</id>
    <summary type="html">{{ .GetIntrotext | html}} <a href="http://ljz.mx/{{ .url }}">Leer m√°s...</a></summary>
		<content type="html">{{ .content | html }}  </content>
    
    <author><name>{{if .author }}{{ .author }}{{ end }} </name></author>
    <category term="{{.GetCategories}}" />
	</entry>
	{{ end }}
</feed>
